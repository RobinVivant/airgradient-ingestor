{
	"name": "airgradient",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "rollup -c",
		"deploy": "export NO_D1_WARNING=true && npm run build && wrangler deploy",
		"start": "export NO_D1_WARNING=true && npm run build && wrangler dev",
		"tail": "export NO_D1_WARNING=true && wrangler tail --format pretty",
		"db-init": "wrangler d1 execute airgradient --file src/db-init.sql"
	},
	"dependencies": {
		"@hono/zod-validator": "^0.1.3",
		"hono": "^3.3.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^25.0.3",
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-babel": "^6.0.3",
		"@babel/core": "^7.22.9",
		"@babel/preset-react": "^7.22.5",
		"rollup": "^3.26.3",
		"rollup-plugin-string": "^3.0.0",
		"wrangler": "^3.3.0"
	}
}
